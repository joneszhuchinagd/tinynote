{
  "name": "tinynote",
  "version": "1.1.2",
  "description": "小注释生成api文档",
  "main": "bin/tinynote.js",
  "bin": {
    "tinynote": "bin/tinynote.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Weijie Zhu <443984744@qq.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joneszhuchinagd/tinynote.git"
  },
  "keywords": [
    "note",
    "api",
    "doc",
    "router",
    "generate"
  ],
  "tinynote": {
    "dist": "test/echo/api.md",
    "source": "test/temp/*.js",
    "template": [{
        "type": "Line",
        "name": "title",
        "template": "## ${text}"
      },
      {
        "type": "Line",
        "name": "url",
        "template": "#### 地址 : ${text}"
      },
      {
        "type": "Line",
        "name": "method",
        "template": "#### 方法 : ${text}"
      },
      {
        "type": "Table",
        "name": "params",
        "title": "#### 参数 :",
        "th": [
          "参数名",
          "类型",
          "必填",
          "说明"
        ]
      },
      {
        "type": "Table",
        "name": "code",
        "title": "#### 返回码 :",
        "th": [
          "返回码",
          "说明"
        ]
      },
      {
        "type": "Code",
        "name": "return",
        "title": "#### 返回值 :"
      }
    ]
  }
}